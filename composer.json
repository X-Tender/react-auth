{
	"name": "codeplaylove/slim-auth",
	"description": "Experimental PHP stack with slim autorization and route nested data load",
	"require": {
		"slim/slim": "^4.8",
		"symfony/var-dumper": "^5.3",
		"vlucas/phpdotenv": "^5.3",
		"slim/psr7": "^1.4",
		"slim/twig-view": "^3.2",
		"league/container": "^3.3",
		"slim/http": "^1.2",
		"illuminate/database": "^8.49",
		"robmorgan/phinx": "^0.12.7",
		"slim/flash": "^0.4.0",
		"tuupola/slim-jwt-auth": "^3.5",
		"dflydev/dot-access-data": "^3.0",
		"dflydev/fig-cookies": "^3.0",
		"firebase/php-jwt": "^5.4",
		"zeuxisoo/slim-whoops": "^0.7.3"
	},
	"config": {
		"vendor-dir": "vendor"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"scripts": {
		"post-root-package-install": ["php -r \"file_exists('.env') || copy('.env.example', '.env');\""]
	}
}
